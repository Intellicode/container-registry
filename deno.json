{
  "name": "container-registry",
  "version": "0.1.0",
  "exports": "./main.ts",
  "tasks": {
    "start": "deno run --allow-net --allow-read --allow-write --allow-env main.ts",
    "dev": "deno run --watch --allow-net --allow-read --allow-write --allow-env main.ts",
    "test": "deno test --allow-net --allow-read --allow-write --allow-env"
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@1",
    "@std/bytes": "jsr:@std/bytes@^1.0.6",
    "@std/crypto": "jsr:@std/crypto@^1.0.0",
    "@std/fs": "jsr:@std/fs@^1.0.22",
    "@std/path": "jsr:@std/path@^1.1.4",
    "@std/encoding": "jsr:@std/encoding@^1.0.10",
    "@std/uuid": "jsr:@std/uuid@^1.0.0",
    "bcrypt": "https://deno.land/x/bcrypt@v0.4.1/mod.ts",
    "djwt": "https://deno.land/x/djwt@v3.0.2/mod.ts",
    "hono": "jsr:@hono/hono@4.11.7",
    "hono/logger": "jsr:@hono/hono@4.11.7/logger"
  },
  "compilerOptions": {
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true
  }
}
